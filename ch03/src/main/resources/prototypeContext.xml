<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="prototypeBean" class="basic.PrototypeBean" scope="prototype"/>
    <bean id="normalBean" class="basic.NormalBean"/>

    <bean id="singletonBean" class="basic.SingletonBean">
        <lookup-method name="getPrototypeBean" bean="prototypeBean"/>
    </bean>
</beans>